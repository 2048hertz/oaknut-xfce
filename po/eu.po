#: ../panel/main.c:55
msgid ""
" OPTIONS\n"
" -h, --help      Show this message and exit\n"
" -v, --version   Show this message and exit\n"
" -c, --customize Show configuration dialog\n"
" -s, --save      Save panel configurations\n"
" -r, --restart   Restart panels\n"
" -q, --quit      End the session\n"
" -x, --exit      Exit panels\n"
" -a, --add       Add new items\n"
"\n"
msgstr ""
" AUKERAK\n"
" -h, --help\t  Mezu hau bistarazi eta irten\n"
" -v, --version\t  Mezu hau bistarazi eta irten\n"
" -c, --customize Konfigurazio elkarrizketa bistarazi\n"
" -s, --save \t  Panel konfigurazioa gorde\n"
" -r, --restart   Panelak berrabiarazi\n"
" -q, --quit      Saio amaitu\n"
" -x, --exit      Panelak itxi\n"
" -a, --add       Elementu berria gehitu\n"

