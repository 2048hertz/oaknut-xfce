#: ../panel/main.c:52
#, c-format
msgid ""
" Usage: %s [OPTIONS]\n"
"\n"
msgstr ""
" 使用法: %s [オプション]\n"
"\n"

#. Only translate the descriptions, not the options itself
#: ../panel/main.c:55
msgid ""
" OPTIONS\n"
" -h, --help      Show this message and exit\n"
" -v, --version   Show this message and exit\n"
" -c, --customize Show configuration dialog\n"
" -s, --save      Save configuration\n"
" -r, --restart   Restart panels\n"
" -q, --quit      End the session\n"
" -x, --exit      Close all panels and end the program\n"
" -a, --add       Add new items\n"
"\n"
msgstr ""
" オプション\n"
" -h, --help      このメッセージを表示して終了します\n"
" -v, --version   このメッセージを表示して終了します\n"
" -c, --customize 設定ダイアログを表示します\n"
" -s, --save      パネルの設定を保存します\n"
" -r, --restart   パネルを再起動します\n"
" -q, --quit      このセッションを終了します\n"
" -x, --exit      全てのパネルを閉じてこのプログラムを終了します\n"
" -a, --add       新しいアイテムを追加します\n"
"\n"

